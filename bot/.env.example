# Astrospheric Pro API key (get from astrospheric.com -> My Profile -> API Key)
ASTROSPHERIC_API_KEY=your_api_key_here

# Telegram bot token (get from @BotFather)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Telegram chat ID (group ID is negative, e.g., -123456789)
TELEGRAM_CHAT_ID=your_chat_id_here

# Observing location coordinates
LATITUDE=37.7749
LONGITUDE=-122.4194

# Sentry DSN for error tracking
SENTRY_DSN=your_sentry_dsn_here
